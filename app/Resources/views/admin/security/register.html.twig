{% extends "baseAdmin.html.twig" %}

{% block body %}
<div class="container">
    <div class="row">
        <div class="col-lg-12">
        {{ form_start(form, {"attr": {"novalidate": "novalidate"}}, {"class": "img-fluid"})}}
        {{ form_row(form.username) }}
        {{ form_row(form.email) }}
        {{ form_row(form.plainPassword) }}
        {{ form_row(form.fullName) }}
        {{ form_end(form) }}
      </div>
    </div>
</div>
{% endblock %}