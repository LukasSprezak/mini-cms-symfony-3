{% extends 'base.html.twig' %}
{% block body %}
<div class="container">
    <div class="row">
        <div class="col-md-8">
            <div class="card mb-4">
                <img class="card-img-top" src="http://placehold.it/750x300" alt="Card image cap">
                <div class="card-body">

                    <h1>{{ page.title }}</h1>
                    <p class="card-text">{{ page.content|raw }}</p>

                </div>
                <div class="card-footer text-muted">
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}